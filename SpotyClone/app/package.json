{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max_old_space_size=4096\" ./node_modules/env-cmd/bin/env-cmd.js -f env/.env.dev ./node_modules/.bin/next dev",
    "build:dev": "./node_modules/.bin/env-cmd -f env/.env.dev ./node_modules/.bin/next build",
    "build:prod": "./node_modules/.bin/env-cmd -f env/.env.prod ./node_modules/.bin/next build",
    "start": "node_modules/.bin/next start",
    "lint": "eslint \"src/**/*.{js,jsx,tsx,ts}\" --fix --quiet",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "stylelint": "stylelint --fix src/**/*.scss",
    "format": "prettier --write . --ignore-path .gitignore --ignore-path .prettierignore",
    "prettier": "prettier --write . --ignore-path .prettierignore",
    "bundle:dev": "ANALYZE=true npm run build:dev",
    "server:dev": "./node_modules/.bin/env-cmd -f env/.env.dev node ./server/server.js"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "env-cmd": "^10.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "next": "14.1.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5"
  }
}
